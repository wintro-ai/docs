#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/julesvanreempts/Projects/docs/node_modules/.pnpm/astro@4.16.18_typescript@5.8.2/node_modules/astro/node_modules:/Users/julesvanreempts/Projects/docs/node_modules/.pnpm/astro@4.16.18_typescript@5.8.2/node_modules:/Users/julesvanreempts/Projects/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/julesvanreempts/Projects/docs/node_modules/.pnpm/astro@4.16.18_typescript@5.8.2/node_modules/astro/node_modules:/Users/julesvanreempts/Projects/docs/node_modules/.pnpm/astro@4.16.18_typescript@5.8.2/node_modules:/Users/julesvanreempts/Projects/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
